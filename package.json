{
  "name": "vite-plugin-build-console",
  "type": "module",
  "version": "1.0.2",
  "description": "",
  "author": "Abraham K.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/InsHomePgup/vite-plugin-build-console"
  },
  "bugs": "https://github.com/InsHomePgup/vite-plugin-build-console/issues",
  "keywords": [
    "vite",
    "plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm --filter playground-vue-ts dev",
    "build:test": "pnpm --filter playground-vue-ts build",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "boxen": "^8.0.1",
    "bytes-formatter": "^21.6.15",
    "dayjs": "^1.11.13",
    "get-folder-size": "^5.0.0",
    "gradient-string": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@antfu/ni": "^25.0.0",
    "@antfu/utils": "^9.2.0",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-legacy": "7.1.0",
    "bumpp": "^10.2.0",
    "eslint": "^9.32.0",
    "lint-staged": "^16.1.2",
    "pnpm": "^10.13.1",
    "simple-git-hooks": "^2.13.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unbuild": "^3.6.0",
    "vite": "7.0.6",
    "vitest": "^3.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
