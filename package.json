{
  "name": "vite-plugin-build-console",
  "type": "module",
  "version": "1.0.2",
  "description": "",
  "author": "Abraham K.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/InsHomePgup/vite-plugin-build-console"
  },
  "bugs": "https://github.com/InsHomePgup/vite-plugin-build-console/issues",
  "keywords": [
    "vite",
    "plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "boxen": "^8.0.1",
    "bytes-formatter": "^21.6.15",
    "dayjs": "^1.11.13",
    "get-folder-size": "^5.0.0",
    "gradient-string": "^3.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^8.1.1",
    "@types/node": "^22.13.17",
    "@vitejs/plugin-legacy": "2.3.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "pnpm": "^10.7.1",
    "simple-git-hooks": "^2.12.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "6.1.0",
    "vitest": "^3.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint . --fix"
  }
}
